#use <conio>
#use <string>

struct ll_node {
    int val;
    struct ll_node* next;
};

typedef struct ll_node ll_node_t;

void print_node(ll_node_t* L) {
    if (L == NULL) {
        print("NULL");
        return;
    } 
    printf("[%d, ", L -> val);
    print_node(L -> next);
    print("]");
}

int main() {
    ll_node_t* L1 = alloc(ll_node_t);
    ll_node_t* L2 = alloc(ll_node_t);
    L1 -> next = L2;
    L1 -> val = -1;
    L2 -> val = -2;
    // L1 -> next -> next = L1;
    print_node(L1);
    return 0;
}
